  "scripts": {
    "start": "webpack serve --mode development",
    "start-debug": "node-nightly --inspect ./node_modules/webpack/bin/webpack.js serve --mode development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint ./src",
    "lint-style": "stylelint \"**/*.scss\" --custom-syntax postcss-scss",
    "lint-style:fix": "stylelint \"**/*.scss\" --custom-syntax postcss-scss --fix",
    "build-assets:babel-style": "tailwindcss -i ./src/assets/index.css -o ./dist/style.css",
    "build-assets:dev": "webpack --mode development",
    "build-assets:dev-babel": "babel src --out-dir dist --copy-files --no-copy-ignored --watch",
    "build-assets:dev-watch": "webpack --mode development --env watch",
    "build-assets:dev-measure": "webpack --mode development --env measure",
    "build-assets:dev-gzip": "webpack --mode development --env onlyGzip",
    "build-assets:dev-analyze": "webpack --mode development --env onlyAnalyze",
    "build-assets:prod": "webpack --mode production",
    "build-assets:prod-babel": "babel src --out-dir dist --copy-files --no-copy-ignored",
    "build-assets:prod-watch": "webpack --mode production --env watch",
    "build-assets:prod-measure": "webpack --mode production --env measure",
    "build-assets:prod-gzip": "webpack --mode production --env onlyGzip",
    "build-assets:prod-analyze": "webpack --mode production --env onlyAnalyze",
    "storybook": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
