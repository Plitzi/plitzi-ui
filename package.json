{
  "name": "@plitzi/plitzi-ui",
  "version": "0.1.7",
  "homepage": "./",
  "license": "MIT",
  "files": [
    "dist",
    "dist/",
    "dist/node_modules",
    "tailwind.config.js"
  ],
  "type": "module",
  "repository": {
    "type": "https",
    "url": "https://github.com/plitzi/plitzi-ui.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/plitzi"
  },
  "main": "dist/src/index.cjs",
  "module": "dist/src/index.mjs",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.mjs"
    },
    "./tailwind.config": "./tailwind.config.js",
    "./style.css": "./dist/src/plitzi-ui.css",
    "./tailwind/*": {
      "types": "./dist/src/tailwind/*/index.d.ts",
      "import": "./dist/src/tailwind/*"
    },
    "./icons": {
      "types": "./dist/src/icons/index.d.ts",
      "import": "./dist/src/icons/index.mjs"
    },
    "./icons/*": {
      "types": "./dist/src/icons/*/index.d.ts",
      "import": "./dist/src/icons/*"
    },
    "./hooks": {
      "types": "./dist/src/hooks/index.d.ts",
      "import": "./dist/src/hooks/index.mjs"
    },
    "./hooks/*": {
      "types": "./dist/src/hooks/*/index.d.ts",
      "import": "./dist/src/hooks/*"
    },
    "./components": {
      "types": "./dist/src/components/index.d.ts",
      "import": "./dist/src/components"
    },
    "./components/*": {
      "types": "./dist/src/components/*/index.d.ts",
      "import": "./dist/src/components/*"
    },
    "./*": {
      "types": "./dist/src/components/*/index.d.ts",
      "import": "./dist/src/components/*"
    }
  },
  "sideEffects": [
    "*.scss"
  ],
  "dependencies": {
    "@codemirror/autocomplete": "^6.18.4",
    "@codemirror/commands": "^6.8.0",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/search": "^6.5.8",
    "@codemirror/state": "^6.5.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/view": "^6.36.2",
    "@hookform/resolvers": "^3.10.0",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@storybook/addon-actions": "^8.5.2",
    "@storybook/addon-controls": "^8.5.2",
    "@storybook/addon-essentials": "^8.5.2",
    "@storybook/addon-interactions": "8.5.2",
    "@storybook/addon-links": "^8.5.2",
    "@storybook/addon-styling-webpack": "^1.0.1",
    "@storybook/node-logger": "^8.5.2",
    "@storybook/react": "^8.5.2",
    "@storybook/react-vite": "^8.5.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.12.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "cssnano": "^7.0.6",
    "eslint": "9.19.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-storybook": "^0.11.2",
    "flow-bin": "^0.259.1",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.1",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.4.2",
    "sass": "^1.83.4",
    "storybook": "^8.5.2",
    "stylelint": "^16.14.1",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.11.0",
    "tailwindcss": "4.0.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.0.11",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.4",
    "vitest-fetch-mock": "^0.4.3"
  },
  "scripts": {
    "start": "storybook dev -p 6006",
    "build:prod": "tsc -p ./tsconfig.app.json && vite build",
    "build:dev": "tsc -p ./tsconfig.app.json && vite build --mode development --watch --minify false",
    "build:analyze": "npx vite-bundle-visualizer",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint-ts": "tsc",
    "lint-style": "stylelint \"**/*.scss\" --custom-syntax postcss-scss",
    "lint-style:fix": "stylelint \"**/*.scss\" --custom-syntax postcss-scss --fix",
    "build-storybook": "build-storybook"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.6.1+sha512.de524adec81a6c3d7a26d936d439d2832e351cdfc5728f9d91f3fc85dd20b04391c038e9b4ecab11cae2b0dd9f0d55fd355af766bc5c1a7f8d25d96bb2a0b2ca"
}
