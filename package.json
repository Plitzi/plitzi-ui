{
  "name": "@plitzi/plitzi-ui",
  "version": "1.2.15",
  "homepage": "./",
  "license": "MIT",
  "files": [
    "dist",
    "dist/",
    "dist/node_modules",
    "tailwind.config.js"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/plitzi/plitzi-ui.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/plitzi"
  },
  "main": "dist/src/index.cjs",
  "module": "dist/src/index.mjs",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.mjs",
      "require": "./dist/src/index.cjs"
    },
    "./tailwind.config": "./tailwind.config.js",
    "./style.css": "./dist/src/plitzi-ui.css",
    "./tailwind/*": {
      "types": "./dist/src/tailwind/*/index.d.ts",
      "import": "./dist/src/tailwind/*",
      "require": "./dist/src/tailwind/*",
      "default": "./dist/src/tailwind/*"
    },
    "./icons": {
      "types": "./dist/src/icons/index.d.ts",
      "import": "./dist/src/icons/index.mjs",
      "require": "./dist/src/icons/index.cjs"
    },
    "./icons/*": {
      "types": "./dist/src/icons/*/index.d.ts",
      "import": "./dist/src/icons/*",
      "require": "./dist/src/icons/*",
      "default": "./dist/src/icons/*"
    },
    "./hooks": {
      "types": "./dist/src/hooks/index.d.ts",
      "import": "./dist/src/hooks/index.mjs",
      "require": "./dist/src/hooks/index.cjs",
      "default": "./dist/src/hooks/index.cjs"
    },
    "./hooks/*": {
      "types": "./dist/src/hooks/*/index.d.ts",
      "import": "./dist/src/hooks/*",
      "require": "./dist/src/hooks/*",
      "default": "./dist/src/hooks/*"
    },
    "./components": {
      "types": "./dist/src/components/index.d.ts",
      "import": "./dist/src/components",
      "require": "./dist/src/components",
      "default": "./dist/src/components"
    },
    "./components/*": {
      "types": "./dist/src/components/*/index.d.ts",
      "import": "./dist/src/components/*",
      "require": "./dist/src/components/*",
      "default": "./dist/src/components/*"
    },
    "./utils": {
      "types": "./dist/src/utils/index.d.ts",
      "import": "./dist/src/utils/index.mjs",
      "require": "./dist/src/utils/index.cjs"
    },
    "./utils/*": {
      "types": "./dist/src/utils/*/index.d.ts",
      "import": "./dist/src/utils/*",
      "require": "./dist/src/utils/*",
      "default": "./dist/src/utils/*"
    },
    "./*": {
      "types": "./dist/src/components/*/index.d.ts",
      "import": "./dist/src/components/*",
      "require": "./dist/src/components/*",
      "default": "./dist/src/components/*"
    }
  },
  "sideEffects": [
    "**/*.scss"
  ],
  "dependencies": {
    "@codemirror/autocomplete": "^6.20.0",
    "@codemirror/commands": "^6.10.2",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.11",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/search": "^6.6.0",
    "@codemirror/state": "^6.5.4",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.39.12",
    "@hookform/resolvers": "^5.2.2",
    "@uiw/color-convert": "^2.9.4",
    "@uiw/react-color-sketch": "^2.9.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "immer": "^11.1.3",
    "lodash-es": "^4.17.23",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "react-toastify": "^11.0.5",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.4.0",
    "tinycolor2": "^1.6.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "resolutions": {
    "@codemirror/view": "^6.38.1"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-controls": "^9.0.8",
    "@storybook/addon-links": "^10.2.7",
    "@storybook/addon-styling-webpack": "^3.0.0",
    "@storybook/react": "^10.2.7",
    "@storybook/react-vite": "^10.2.7",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.2.1",
    "@types/react": "^19.2.13",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/tinycolor2": "^1.4.6",
    "@vitejs/plugin-react": "^5.1.3",
    "cssnano": "^7.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "eslint-plugin-storybook": "^10.2.7",
    "flow-bin": "^0.299.0",
    "globals": "^17.3.0",
    "jsdom": "^28.0.0",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.8.1",
    "sass": "^1.97.3",
    "storybook": "^10.2.7",
    "stylelint": "^17.1.1",
    "stylelint-order": "^7.0.1",
    "stylelint-scss": "^7.0.0",
    "tailwindcss": "4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.2.0",
    "vitest": "^4.0.18",
    "vitest-fetch-mock": "^0.4.5"
  },
  "scripts": {
    "start": "storybook dev -p 6006",
    "build:prod": "tsc -p ./tsconfig.app.json && vite build",
    "build:dev": "tsc -p ./tsconfig.app.json && vite build --mode development --minify false",
    "build:dev-watch": "tsc -p ./tsconfig.app.json && vite build --mode development --watch --minify false",
    "build:analyze": "npx vite-bundle-visualizer",
    "typecheck": "tsc --noEmit",
    "lint": "TIMING=1 eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint-ts": "tsc",
    "lint-style": "stylelint \"**/*.scss\" --custom-syntax postcss-scss",
    "lint-style:fix": "stylelint \"**/*.scss\" --custom-syntax postcss-scss --fix",
    "build-storybook": "build-storybook"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@4.2.2"
}
